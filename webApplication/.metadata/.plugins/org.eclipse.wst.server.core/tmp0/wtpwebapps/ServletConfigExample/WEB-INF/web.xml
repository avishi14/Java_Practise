  <servlet-mapping>
    <servlet-name>SevletConfig1</servlet-name>
    <url-pattern>/SevletConfig1 </url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TestContext</servlet-name>
    <servlet-class>test2.TestContext</servlet-class>
